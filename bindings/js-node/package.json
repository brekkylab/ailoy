    "description": "",
    "type": "commonjs",
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "types": "dist/index.d.ts",
    "files": [
        "dist",
        "README.md"
    ],
    "scripts": {
        "build:napi": "node build-napi.js",
        "build:ts": "rollup -c --bundleConfigAsCjs",
        "build": "npm run build:napi && npm run build:ts",
        "clean": "rm -rf dist/",
        "test": "ts-mocha",
        "test:debug": "node --inspect-brk ./node_modules/.bin/ts-mocha",
        "prepack": "clean-package",
        "postpack": "clean-package restore"
    },
    "devDependencies": {
        "@rollup/plugin-alias": "^5.1.1",
        "@rollup/plugin-commonjs": "^28.0.3",
        "@rollup/plugin-node-resolve": "^16.0.1",
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^12.1.2",
        "@trivago/prettier-plugin-sort-imports": "^5.2.2",
        "@types/chai": "^5.2.0",
        "@types/jmespath": "^0.15.2",
        "@types/mocha": "^10.0.10",
        "@types/node": "^22.10.7",
        "chai": "^5.2.0",
        "clean-package": "^2.2.0",
        "cmake-js": "^7.3.0",
        "mocha": "^11.2.2",
        "node-addon-api": "^8.3.1",
        "rollup": "^4.40.2",
        "rollup-plugin-copy": "^3.5.0",
        "rollup-plugin-dts": "^6.2.1",
        "rollup-plugin-tsc-alias": "^1.1.4",
        "ts-mocha": "^11.1.0",
        "tslib": "^2.8.1",
        "tsx": "^4.19.3",
        "typescript": "^5.6.3"
    },
    "optionalDependencies": {
        "@rollup/rollup-win32-x64-msvc": "^4.40.2"
    },
    "binary": {
        "napi_versions": [
            7
        ]
    },
    "clean-package": {
        "indent": 2,
        "remove": [
            "scripts",
            "devDependencies",
            "binary",
            "clean-package"
        ]
    },
    "dependencies": {
        "@modelcontextprotocol/sdk": "^1.11.2",
        "jmespath": "^0.16.0"
    }
}
