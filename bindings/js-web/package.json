{
  "name": "ailoy-web",
  "version": "0.1.0",
  "author": "Brekkylab Inc. <contact@brekkylab.com>",
  "license": "Apache-2.0",
  "description": "WASM binding for Ailoy runtime APIs",
  "homepage": "https://brekkylab.github.io/ailoy",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brekkylab/ailoy"
  },
  "type": "module",
  "main": "./dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build:configure": "cd ../.. && cmake --preset emscripten-release",
    "build:wasm": "cd ../.. && cmake --build ./build/emscripten-release -t ailoy_js_web --parallel 10",
    "build:tvm": "make -C ../../build/emscripten-release/_deps/tvm-src/web && cp ../../build/emscripten-release/_deps/tvm-src/web/src/*.js ./src/tvmjs",
    "build:ts": "NODE_ENV=production vite build",
    "build": "npm run build:configure && npm run build:wasm && npm run build:tvm && npm run build:ts",
    "dev": "NODE_ENV=development vite dev",
    "test": "vitest",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/express": "^5.0.3",
    "@types/jmespath": "^0.15.2",
    "@types/node": "^24.2.0",
    "@vitest/browser": "^3.2.4",
    "@webgpu/types": "^0.1.64",
    "clean-package": "^2.2.0",
    "cors": "^2.8.5",
    "esbuild": "^0.25.8",
    "express": "^5.1.0",
    "playwright": "^1.54.2",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-static-copy": "^3.1.1",
    "ws": "^8.18.3",
    "zod": "^3.25.76"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.1",
    "jmespath": "^0.16.0",
    "wasm-vips": "^0.0.14"
  },
  "clean-package": {
    "indent": 2,
    "remove": [
      "scripts",
      "devDependencies",
      "clean-package"
    ]
  }
}
